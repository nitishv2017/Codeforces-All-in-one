<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.codeforces_info">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_arrow"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_arrow_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Codeforces_Info">
        <activity android:name=".Sneak_submissions"></activity>
        <activity android:name=".Friend_profile_view" />
        <activity android:name=".Friend_Profile" />
        <activity android:name=".UsernameEntry" />
        <activity
            android:name=".Authentication"
            android:screenOrientation="portrait" />
        <activity
            android:name=".tabs"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>